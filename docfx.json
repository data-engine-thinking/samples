{
  "build": {
    "content": [
      {
        "files": [
          "/**/*.{md,yml}",
          "toc.yml"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "/**/images/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadata": {
      "_appLogoPath": "images/logo.png",
      "_appTitle": "Data Engine Thinking",
      "_enableSearch": false,
      "_disableTocFilter": true,
      "_disableBreadcrumb": false,
      "_appFooter": "<span>Data Engine Thinking</span>",
      "_disableContribution": true
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/custom"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false,
    "markdownEngineName": "markdig"
  }
}
